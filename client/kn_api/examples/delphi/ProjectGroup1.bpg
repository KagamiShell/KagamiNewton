#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = test.exe test.ENU test.RUS
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

test.exe: test.dpr
  $(DCC)

test.ENU: ENU\test.dpr
  $(DCC)

test.RUS: RUS\test.dpr
  $(DCC)


